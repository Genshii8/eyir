{
  "name": "eyir",
  "version": "0.1.0",
  "type": "module",
  "description": "A Discord bot for the Warrior Discord Community, Skyhold.",
  "repository": "github:adamhl8/eyir",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rm -rf dist && pnpm lint && tsc && pnpm format",
    "docker:push": "pnpm build && docker build . -t ghcr.io/adamhl8/eyir && docker push ghcr.io/adamhl8/eyir:latest",
    "format": "prettier --write .",
    "lint": "eslint -f pretty .",
    "start": "ts-node ./src/index.ts"
  },
  "dependencies": {
    "@discordjs/rest": "^1.2.0",
    "chokidar": "^3.5.3",
    "discord-api-types": "^0.37.12",
    "discord-bot-shared": "^0.6.3",
    "discord.js": "^14.5.0",
    "node-json-db": "^2.1.3"
  },
  "devDependencies": {
    "@swc/core": "^1.3.5",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^44.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-pkg": "^0.17.1",
    "prettier-plugin-sh": "^0.12.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
